<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evandro.tcc.suitecar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TelaInicial"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity
            android:name=".CadastroVeiculo"
            android:label="@string/title_activity_cadastro_veiculo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Combustivel"
            android:label="@string/title_activity_combustivel"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Preventiva"
            android:label="@string/title_activity_preventiva"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CadCombustivel" />
        <activity android:name=".CadPreventiva" />
        <activity
            android:name=".ListaVeiculos"
            android:label="@string/title_activity_lista_veiculos"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>